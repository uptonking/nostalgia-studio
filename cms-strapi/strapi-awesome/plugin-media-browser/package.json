{
  "name": "strapi-plugin-media-browser",
  "version": "0.0.1111",
  "description": "A convenient way to browse, manage and select all your Strapi assets, inspired by MacOS.",
  "keywords": [
    "strapi",
    "files",
    "videos"
  ],
  "license": "MIT",
  "author": "Josh Ellis (https://github.com/joshuaellis)",
  "sideEffects": false,
  "files": [
    "dist",
    "strapi-admin",
    "strapi-server"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:admin && yarn build:server",
    "build:admin": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "clean": "cd demo && yarn clean && cd .. && rm -rf dist node_modules",
    "dev": "rm -rf dist && concurrently \"yarn dev:admin\" \"yarn dev:server\"",
    "dev:admin": "vite build --watch",
    "dev:server": "tsc -p tsconfig.server.json -w",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "yarn test:ts",
    "test:ts": "yarn test:ts:admin && yarn test:ts:server",
    "test:ts:admin": "tsc -p tsconfig.json --noEmit",
    "test:ts:server": "tsc -p tsconfig.server.json --noEmit"
  },
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-toolbar": "^1.0.3",
    "@react-spring/web": "^9.7.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@strapi/design-system": "^1.6.6",
    "@strapi/helper-plugin": "^4.21.0",
    "@strapi/icons": "^1.6.6",
    "@strapi/utils": "^4.0.0",
    "axios": "^1.3.4",
    "fs-extra": "^11.1.0",
    "koa-range": "^0.3.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "5.0.6",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-redux": "8.0.5",
    "react-virtuoso": "^4.0.3",
    "sonner": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@strapi/database": "^4.0.0",
    "@strapi/strapi": "^4.0.0",
    "@strapi/typescript-utils": "^4.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-range": "^0.3.2",
    "@types/koa-static": "^4.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "koa": "^2.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.9"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.3.0"
  },
  "strapi": {
    "name": "media-browser",
    "description": "A convenient way to browse, manage and select all your Strapi assets, inspired by MacOS."
  }
}
