{
  "name": "@floating-ui/utils",
  "version": "0.2.1111",
  "description": "Utilities for Floating UI",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "dom"
  ],
  "scripts": {
    "lint": "biome lint .",
    "clean": "rimraf dist out-tsc dom react",
    "test": "vitest run --globals",
    "test:watch": "vitest watch --globals",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:api": "build-api --tsc tsconfig.lib.json --aec api-extractor.json --aec api-extractor.dom.json --aec api-extractor.react.json",
    "publint": "publint",
    "typecheck": "tsc -b"
  },
  "author": "atomiks",
  "license": "MIT",
  "bugs": "https://github.com/floating-ui/floating-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/floating-ui/floating-ui.git",
    "directory": "packages/utils"
  },
  "homepage": "https://floating-ui.com",
  "keywords": [
    "tooltip",
    "popover",
    "dropdown",
    "menu",
    "popup",
    "positioning"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6"
  }
}
