{
  "name": "pouchdb-changes-filter",
  "version": "8.0.1111",
  "description": "Adds \"filter\", \"selector\", etc. to PouchDB's changes/replicate/sync APIs",
  "main": "./lib/index.js",
  "keywords": [],
  "author": "Dale Harvey <dale@arandomurl.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pouchdb/pouchdb.git",
    "directory": "packages/node_modules/pouchdb-changes-filter"
  },
  "module": "./lib/index.js",
  "browser": {
    "./lib/index.js": "./lib/index-browser.js",
    "./src/evalFilter.js": "./src/evalFilter-browser.js",
    "./src/evalView.js": "./src/evalView-browser.js"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run clean ",
    "build:dev1": "npm run clean && npm run build:dev-web && npm run build:dev-node",
    "build:dev-node": "rspack build -c ./webpacker/rspack.config.dev.node.mjs",
    "build:dev-web": "rspack build -c ./webpacker/rspack.config.dev.web.mjs",
    "build:prod": "npm run clean && rspack build -c ./webpacker/rspack.config.prod.mjs",
    "build:tsc": "npm run clean && tsc -p tsconfig.json -w",
    "clean": "rimraf lib *.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "pouchdb-utils": "8.0.1111",
    "pouchdb-selector-core": "8.0.1111"
  },
  "devDependencies": {
    "@types/node": "^20.11.19"
  }
}
